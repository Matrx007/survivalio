    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "npm-watch": "^0.7.0",
    "rxjs": "^6.6.3"
